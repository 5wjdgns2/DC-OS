curl -k -O https://bootstrap.pypa.io/get-pip.py

python get-pip.py

install setuptools

wget https://files.pythonhosted.org/packages/f7/b6/5b98441b6749ea1db1e41e5e6e7a93cbdd7ffd45e11fe1b22d45884bc777/setuptools-42.0.2.zip

